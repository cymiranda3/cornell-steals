  border: 1px solid white;\n    border-radius: 3px;\n    padding: 10px;\n    font-family: 'Roboto';\n    font-size: 2vw;\n    font-weight: 100;\n    color: white;\n    -webkit-animation: glowing 4500ms infinite;\n    -moz-animation: glowing 4500ms infinite;\n    -o-animation: glowing 4500ms infinite;\n    animation: glowing 4500ms infinite;\n}\n@-webkit-keyframes glowing {\n  0% { -webkit-box-shadow: 0 0 3px #B20000; }\n  50% { -webkit-box-shadow: 0 0 40px #FF0000; }\n  100% { -webkit-box-shadow: 0 0 3px #B20000; }\n}\n\n@-moz-keyframes glowing {\n  0% { -moz-box-shadow: 0 0 3px #B20000; }\n  50% { -moz-box-shadow: 0 0 40px #FF0000; }\n  100% {  -moz-box-shadow: 0 0 3px #B20000; }\n}\n\n@-o-keyframes glowing {\n  0% {  box-shadow: 0 0 3px #B20000; }\n  50% {  box-shadow: 0 0 40px #FF0000; }\n  100% {  box-shadow: 0 0 3px #B20000; }\n}\n\n@keyframes glowing {\n  0% { box-shadow: 0 0 3px #B20000; }\n  50% { box-shadow: 0 0 40px #FF0000; }\n  100% { box-shadow: 0 0 3px #B20000; }\n}\n\n  * {\n    box-sizing: border-box;\n}\n\n/* Create two equal columns that floats next to each other */\n.column {\n    float: left;\n    width: 50%;\n    padding: 10px;\n    background-color: transparent;\n}\n\n/* Clear floats after the columns */\n.row:after {\n    content: \"\";\n    display: table;\n    clear: both;\n}\n\n#column1 {\n  text-align: left;\n}\n\n#column2 {\n  margin-top: 2vw;\n  padding-left: 10vw;\n  text-align: left;\n  margin-bottom: 200px;\n}\n\n#product_image {\n    margin-left: 8vw;\n    margin-right: auto;\n    padding-left: 5vw;\n    padding-right: 5vw;\n    height: 32vw;\n    width: auto;\n    background-image: linear-gradient(pink, blue);\n    border-radius: 60px;\n    box-shadow: 4vw 4vw white;\n  }\n\n  #product_info {\n    margin-left: 2vw;\n  }\n\n  #countdown {\n    font-family: 'Roboto';\n    font-weight: 100;\n    color: white;\n    font-size: 2vw;\n  }\n\n  #remaining {\n    font-family: 'Roboto';\n    font-size: 2vw;\n  }\n\n  @media screen and (max-width: 600px) {\n    .column {\n      width: 100vw;\n    }\n    #column1 img {\n      width: 66vw;\n      height: auto;\n      display: block;\n      margin-left: auto;\n      margin-right: auto;\n    }\n    #column2 {\n      margin-bottom: 300px;\n      text-align: center;\n      margin-left: 0;\n      padding-left: 0;\n    }\n    #product-name {\n      font-size: 10vw;\n      margin-bottom: 1vw;\n    }\n    #price, #original-price {\n      font-size: 8vw;\n    }\n    #countdown {\n      font-size: 6vw;\n    }\n    #remaining {\n      font-size: 6vw;\n    }\n    button {\n      margin-top: 10vw;\n      font-size: 8vw;\n    }\n  }\n</style>\n"]}]}