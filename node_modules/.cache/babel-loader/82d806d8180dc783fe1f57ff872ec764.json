er = true;\n      }\n    }\n  },\n  data (){\n    return {\n      showShop: false,\n      showPasswordArea: true,\n      showLargelogoArea: true,\n      showHeader: false,\n      router: false,\n    }\n  }\n}\n</script>\n\n<style>\nhtml {\n  background-color: #2e3131;\n}\nbody {\n  margin: 0;\n}\n#header {\n  text-align: left;\n  background-color: black;\n  padding-bottom: .75vw;\n  width: 100%;\n  margin: 0;\n}\n#logo {\n  margin-top: 20px;\n  width: 40vw;\n}\n#smallLogo {\n  margin-left: 2vw;\n  margin-top: 1vw;\n  height: auto;\n  width: 12vw;\n}\n\n/* NAVBAR */\n.navbar {\n    list-style-type: none;\n    margin: 0;\n    padding: 0;\n    overflow: hidden;\n    background-color: #333;\n    display: inline;\n}\n\n.navbar li {\n    float: right;\n    width: 10vw;\n    min-width: 10vw;\n    padding-top: 1vw;\n}\n\n.navbar li a {\n    display: block;\n    color: white;\n    text-align: center;\n    padding: 14px 16px;\n    text-decoration: none;\n    font-size: 2vw;\n}\n\n#app {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n#shopArea {\n  margin-top: 4vw;\n}\n\n@media screen and (max-width: 600px) {\n  header {\n    height: 30vw;\n    text-align: center;\n  }\n  #smallLogo {\n    width: 40vw;\n    display: block;\n    margin-left: auto;\n    margin-right: auto;\n    padding-top: 2vw;\n  }\n  \n  .navbar {\n    display: block;\n    width: auto;\n    padding-left: 12vw;\n    padding-right: auto;\n    background-color: black;\n  }\n  .navbar li {\n    flex-direction: row;\n    float: left;\n    width: 25vw;\n    align-self: center;\n    align-items: center;\n    text-align: center;\n    align-content: center;\n  }\n  .navbar li a {\n    font-size: 5vw;\n  }\n\n}\n</style>\n"],"sourceRoot":"src"}]}